  "components": {
    "schemas": {
      "Client": {
        "required": [
          "name"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "maxLength": 200,
            "type": "string",
            "format": "email",
            "nullable": true
          },
          "phoneNumber": {
            "maxLength": 30,
            "type": "string",
            "nullable": true
          },
          "preferredGoal": {
            "$ref": "#/components/schemas/FitnessGoal"
          },
          "budgetMin": {
            "type": "number",
            "format": "double"
          },
          "budgetMax": {
            "type": "number",
            "format": "double"
          },
          "homeLatitude": {
            "type": "number",
            "format": "double"
          },
          "homeLongitude": {
            "type": "number",
            "format": "double"
          },
          "address": {
            "maxLength": 300,
            "type": "string",
            "nullable": true
          },
          "preferredGym": {
            "maxLength": 100,
            "type": "string",
            "nullable": true
          },
          "goals": {
            "maxLength": 1000,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Coach": {
        "required": [
          "name",
          "specialization"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "specialization": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "bio": {
            "maxLength": 2000,
            "type": "string",
            "nullable": true
          },
          "yearsExperience": {
            "type": "integer",
            "format": "int32"
          },
          "certifications": {
            "maxLength": 500,
            "type": "string",
            "nullable": true
          },
          "coachGyms": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CoachGym"
            },
            "nullable": true
          },
          "plans": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TrainingPlan"
            },
            "nullable": true
          },
          "portfolio": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PortfolioPost"
            },
            "nullable": true
          },
          "reviews": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Review"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CoachGym": {
        "type": "object",
        "properties": {
          "coachId": {
            "type": "integer",
            "format": "int32"
          },
          "coach": {
            "$ref": "#/components/schemas/Coach"
          },
          "gymId": {
            "type": "integer",
            "format": "int32"
          },
          "gym": {
            "$ref": "#/components/schemas/Gym"
          },
          "availableDays": {
            "type": "string",
            "nullable": true
          },
          "startTime": {
            "type": "string",
            "format": "date-span"
          },
          "endTime": {
            "type": "string",
            "format": "date-span"
          },
          "hourlyRate": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
