        "enum": [
          "GeneralFitness",
          "WeightLoss",
          "MuscleGain",
          "Endurance",
          "Flexibility",
          "Rehab"
        ],
        "type": "string"
      },
      "Gym": {
        "required": [
          "location",
          "name"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
